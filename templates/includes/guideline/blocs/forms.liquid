	<div class="col-sm-4 ">
		<h3>form search</h3>
		<p>Text</p>
	</div>
	<div class="col-sm-8">
		<form class="form-search">
			<div class="input-append">
				<input type="text" class="span2 search-query">
				<button type="submit" class="btn">Search</button>
			</div>
			<div class="input-prepend">
				<button type="submit" class="btn">Search</button>
				<input type="text" class="span2 search-query">
			</div>
		</form>
	</div>
</div>
